{
  "name": "r-pipeline",
  "version": "1.0.0",
  "type": "module",
  "description": "A TypeScript utility library for data processing pipelines",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest --coverage",
    "test:watch": "vitest --coverage --watch",
    "test:ui": "vitest --coverage --ui",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "typescript",
    "pipeline",
    "data-processing",
    "utility"
  ],
  "author": "riskrisk",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vitest": "^0.2.0",
    "prettier": "^3.0.0",
    "rollup": "^3.25.0",
    "rollup-plugin-dts": "^5.3.0",
    "tslib": "^2.6.0",
    "tsx": "^4.20.6",
    "typescript": "^5.1.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riskrisk/r-pipeline.git"
  },
  "bugs": {
    "url": "https://github.com/riskrisk/r-pipeline/issues"
  },
  "homepage": "https://github.com/riskrisk/r-pipeline#readme"
}
